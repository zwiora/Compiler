#[ Wymnozenie liczby 340282367713220089251654026161790386200 
#  ze stalych 33-bitowych
#  ( (2^32+1) * (2^32+2) * (2^32+3) * (2^32+4) )
#  na 2 sposoby.
#]
PROGRAM IS
    a[4],
    n, m
IN
    a[0] := 4294967297;
    a[1] := 4294967298;
    a[2] := 4294967299;
    a[3] := 4294967300;

    n := a[0] * a[1];
    n :=   n  * a[2];
    n :=   n  * a[3];
    WRITE n;

    n := a[0] * a[3];
    m := a[1] * a[2];
    n := n * m;
    WRITE n;

END
